<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ImageUploadController">
   <children>
      <ImageView fx:id="ivImage" fitHeight="108.0" fitWidth="138.0" layoutX="44.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../JavaFX/LibraryManagementSystem/src/image/user.png" />
         </image>
      </ImageView>
      <Button fx:id="btnBrowse" layoutX="47.0" layoutY="140.0" mnemonicParsing="false" onAction="#processBrowse" prefHeight="25.0" prefWidth="102.0" text="Browse" />
      <Button fx:id="btnSave" layoutX="294.0" layoutY="24.0" mnemonicParsing="false" onAction="#processSave" prefHeight="25.0" prefWidth="102.0" text="Save" />
      <TextArea fx:id="taImagePath" layoutX="34.0" layoutY="249.0" prefHeight="69.0" prefWidth="233.0" promptText="Path of Image File" />
      <Button fx:id="btnUpdate" layoutX="316.0" layoutY="271.0" mnemonicParsing="false" onAction="#processUpdate" prefHeight="25.0" prefWidth="108.0" text="Update" />
      <Button fx:id="btnDtail" layoutX="316.0" layoutY="200.0" mnemonicParsing="false" onAction="#processDetail" prefHeight="25.0" prefWidth="108.0" text="Detail" />
   </children>
</AnchorPane>
